{
  "name": "toduo",
  "version": "1.0.50",
  "scripts": {
    "dev": "bun run --watch src/index.ts & cd frontend && bun run dev",
    "build": "npm run build-frontend && npm run build-backend",
    "build-frontend": "cd frontend && bun run build",
    "build-backend": "bun build --target=bun src/index.ts --outdir dist",
    "start": "bun run dist/index.js",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@elysiajs/static": "^1.1.1",
    "elysia": "latest"
  },
  "devDependencies": {
    "bun-types": "latest",
    "prettier": "^3.3.3"
  },
  "module": "src/index.js"
}
